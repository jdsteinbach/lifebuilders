---
layout: _base
---
<div class="pages-list">
  {% for cat in collections.nav %}
    <h2><a href="{{ cat.url }}">{{ cat.data.title }}</a></h2>
    <ul>
      {% for link in collections[cat.data.tags] %}
        {% if link.data.title == cat.data.title %}
          {% continue %}
        {% endif %}
        <li>
          <a href="{{ link.url }}">{{ link.data.title }}</a>
        </li>
      {% endfor %}
    </ul>
  {% endfor %}
</div>
